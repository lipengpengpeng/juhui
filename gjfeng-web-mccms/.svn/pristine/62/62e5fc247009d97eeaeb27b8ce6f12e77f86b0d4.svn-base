/**
  *ddaccordion菜单插件样式
  */
.arrowlistmenu{
	margin:5px;
}
.arrowlistmenu .menuheader{ /*CSS class for menu headers in general (expanding or not!)*/
background-image:url(menu.gif);
width:190px;
height:26px;
font-size:12px;
cursor: pointer;
margin-bottom:3px;
}
.arrowlistmenu .openheader{ /*CSS class to apply to expandable header when it's expanded*/
background-image:url(menu_click.gif);
width:190px;
height:26px;
}
.arrowlistmenu ul{ /*CSS for UL of each sub menu*/
list-style-type: none;
margin: 0;
padding: 0;
}
.arrowlistmenu ul li{
color:#000000;
display: block;
padding: 2px 0;
padding-left: 5px; /*link text is indented 19px*/
text-decoration: none;
font-size: 90%;
cursor:pointer;
margin-bottom:2px;
}
.arrowlistmenu ul li:hover{ /*hover state CSS*/
color: #A70303;
background-color: #F3F3F3;
}
.arrowlistmenu .menuheader .menu_title
{
padding-left:5px;
padding-top:8px;
}
.arrowlistmenu .menu_icon{
width:14px;
height:11px;
background-image:url("menu_icon.gif");
background-repeat:no-repeat;
float:left;
}
.arrowlistmenu .menu_item_icon{
width:14px;
height:11px;
background-image:url("menu_item_icon.gif");
background-repeat:no-repeat;
float:left;
}


/*tabs*/
#tabs
{
position: absolute;
left: 0px;
top:0px;
}
#tabsFrame
{
/*background-image:url("tabs.jpg");*/
overflow: hidden;
}
.ui-tabs-panel
{
border-width:0px;
height:100%;
padding:0px;
}
.tabsCloseButton
{
width:10px;
height:10px;
background:url("tabs_close.gif");
margin:5px 0 0 0;
overflow: hidden;
}
#tabsMove
{
	width:60px;height:22px;background:url("tabs.jpg");position: absolute;left:100%;top:0px;margin:0 0 0 -60px;
}
#rightMove
{
	width:20px;
	height:20px;
	background:url("tabs_right.gif");
	position: absolute;
	top:2px;
	left:35px;
	cursor: pointer;
}
#leftMove
{
	width:20px;height:20px;background:url("tabs_left.gif");position: absolute;top:2px;left:5px;cursor: pointer;
}

/*tabs init*/
#tabs
{
	/*width:3000%;*/
	width:100%
}
#tabs .img, .gap
{
	background:url("gap.jpg");
	width:7px;
	height:32px;
}
.ui-tabs-hide
{
	display: none !important;
}
.ui-tabs-nav
{
	list-style: none;
	margin:0px;
	padding:0px;
	position: absolute;
}
.ui-tabs-nav a:link {color: #000;text-decoration: none;}
.ui-tabs-nav a:visited {color: #000;text-decoration: none;}
.ui-tabs-nav a:hover {color: #000;text-decoration: none;}
.ui-tabs-nav a:active {color: #000;text-decoration: none;}
.ui-tabs-nav li
{
	float:left;
	margin:0 1px 0 1px;
	line-height: 22px;vertical-align: middle;
}
.ui-tabs-selected
{
	background:url("tabs.gif");
}
.ui-tabs-nav li a,.ui-tabs-nav li div
{
	float:left;
}


/*layout button*/
#layout-top-open
{
	background:url("layout_down.gif");width:16px;height:16px;
	position: absolute;left:100%;margin:0 0 0 -16px;
	cursor: pointer;
	display: none;
}
#layout-menu-open
{
	background:url("layout_right.gif");width:16px;height:16px;
	position: absolute;top:100%;left:4px;margin:-16px 0 0 0;
	cursor: pointer;
	display: none;
}
#layout-footer-open
{
	background:url("layout_up.gif");width:16px;height:16px;
	position: absolute;left:100%;top:100%;margin:-16px 0 0 -16px;
	cursor: pointer;
	display: none;
}
#layout-top-close
{
	background:url("layout_up.gif");width:16px;height:16px;
	position: absolute;left:100%;top:0px;margin:0 0 0 -16px;
	cursor: pointer;
}
#layout-menu-close
{
	background:url("layout_left.gif");width:13px;height:13px;
	position: absolute;top:2px;left:4px;
	cursor: pointer;
}
#layout-footer-close
{
	background:url("layout_down.gif");width:16px;height:16px;
	position: absolute;left:100%;top:50%;margin:-8px 0 0 -16px;
	cursor: pointer;
}

