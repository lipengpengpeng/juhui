/*my-center*/
.nav-footer{position: fixed;bottom: 0;left: 0;width: 100%;height: 5rem;background-color: #fff;border-top: 1px solid #ddd;}
.nav-footer nav{width: 100%;height: 100%;}
.nav-link{display: inline-block;vertical-align: middle;width: 25%;text-align: center;font-size:1.2rem;float: left;padding-top: 0.5rem;height: 100%;}
.nav-icon{background: url('../../../image/wx/o2o-shop/my-sprite.png') no-repeat;display: inline-block;background-size:30rem 37.5rem;}
.icon-nav-1,.icon-nav-2,.icon-nav-3,.icon-nav-4{height: 2.25rem;}
.icon-nav-1{width: 2.2rem;background-position: 0 bottom;}
.nav-link-active .icon-nav-1{background-position: -2.6rem bottom;}
.icon-nav-2{width: 2.4rem;background-position: -5.2rem bottom;}
.nav-link-active .icon-nav-2{background-position: -7.95rem bottom;}
.icon-nav-3{width: 2.2rem;background-position: -10.75rem bottom;}
.nav-link-active .icon-nav-3{background-position: -13.3rem bottom;}
.icon-nav-4{width: 2.15rem;background-position: -15.9rem bottom;}
.nav-link-active .icon-nav-4{background-position: -18.4rem bottom;}
.nav-text{padding-top: 0.3rem;color:#707070;}
.nav-link-active .nav-text{color:#ff5350;}
.cont-top{position: relative;}
.img-head{width: 6.5rem;height: 6.5rem;border-radius: 6rem;border:3px solid rgba(255,255,255,.5);position: relative;z-index: 100;}
.top-banner{width: 100%;height: 7.5rem;background:url('../../../image/wx/o2o-shop/mine_background.png') 0 100% no-repeat;-webkit-background-size: cover;
background-size: cover;position: relative;}
.img-cover{position: absolute;top:42%;left: 50%;margin-left: -3.25rem;}
.img-sex{position: absolute;left:-2.5rem;top:1.5rem;display: inline-block;background-color: #f860ef;color:#fff;font-size: 1.2rem;padding:0.5rem 2rem 0.5rem 1rem;z-index:90;border-top-left-radius:1rem;border-bottom-left-radius: 0.3rem;}
.recommend-name-box{position: relative;left:-3.2rem;top: -2.3rem;padding: 0.2rem 0.5rem 0.2rem 2.5rem;display: inline-block;background-color: #ff8432;color:#fff;font-size: 1.2rem;z-index: 90;border-top-right-radius: 0.3rem;border-bottom-right-radius: 1rem;}
.recommend-name{max-width: 6.7rem;overflow:hidden;text-overflow: ellipsis;white-space: nowrap;padding: 0.3rem;display: inline-block;vertical-align: top;}
.recommend-tip{border-radius:50%;border:1px solid #fff;margin-left: 0.2rem;padding:0.2rem;font-size: 0.9rem;display: inline-block;vertical-align: top;}
.top-text{background-color: #fff;}
.top-name{padding-top: 2.8rem;font-size: 1.6rem;color:#333;text-align: center;}
.top-abstract{color:#999;font-size: 1.2rem;text-align: center;padding: 0 6rem;line-height: 1.5;margin-top: 0.5rem;padding-bottom: 1rem;}
.cont-order{background-color: #fff;width: 100%;margin-top: 1.1rem;}
.my-order{padding:0.6rem 1rem;border-bottom:1px solid #ddd;}
.check-order a{color:#999;font-size: 1.2rem;line-height: 2.4;display: inline-block;}
.orderNav-link{width: 25%;text-align: center;}
.my-icon{background-size:30rem 37.5rem;background-repeat: no-repeat;background-image:url('../../../image/wx/o2o-shop/my-sprite.png');}
.icon-more{width: 0.7rem;height: 1.2rem;background-position:right 0;vertical-align: text-top;margin-left: 0.7rem;}
.order-icon-1{width:2.3rem;height: 1.9rem;background-position: -13.25rem 0;}
.order-icon-2{width:2.3rem;height: 2rem;background-position:-13.25rem -2.25rem;}
.order-icon-3{width:2.3rem;height: 1.8rem;background-position: -13.25rem -4.7rem;}
.order-icon-4{width:2.3rem;height: 2rem;background-position: -13.25rem -7.05rem;}
.orderNav-link i{height: 1.9rem;margin:1.5rem 0 0.8rem;}
.order-text{color:#333;font-size: 1.2rem;margin-bottom: 1rem;}
.contMenu-box{margin-top: 1.1rem;background-color: #fff;border-top:1px solid #ddd;border-bottom:1px solid #ddd;}
.contMenu-text{font-size: 1.4rem;display: inline-block;margin-left: 1rem;}
.contMenu-icon,.main-sprite{vertical-align: middle;background-size:30rem 37.5rem;background-image: url('../../../image/wx/o2o-shop/my-sprite.png');background-repeat: no-repeat;}
.contMenu-icon-1{width: 2.1rem;height: 1.9rem;background-position:-9.3rem 0;}
.contMenu-icon-2{width: 2.1rem;height: 2rem;background-position: -9.3rem -2.2rem;}
.contMenu-icon-3{width: 2.1rem;height: 1.9rem;background-position: -9.3rem -4.5rem;}
.contMenu-icon-4{width: 2.1rem;height: 1.9rem;background-position: -9.3rem -6.7rem;}
.contMenu-icon-5{width: 2.1rem;height: 2.1rem;background-position: -9.3rem -8.85rem;}
.contMenu-icon-6{width: 2.1rem;height: 1.9rem;background-position: -9.3rem -11.25rem;}
.contMenu-icon-7{width: 2.1rem;height: 2.1rem;background-position: -9.3rem -13.4rem;}
.contMenu-icon-8{width: 2.1rem;height: 2.1rem;background-position: -9.3rem -15.85rem;}
.contMenu-icon-9{width: 2.1rem;height: 2.2rem;background-position: -9.3rem -18.3rem;}
.contMenu-icon-10{width: 2.1rem;height: 2.1rem;background-position: -13.3rem -15rem;}
.contMenu-box li+li{border-top: 1px solid #ddd;}
.contMenu-box li a{padding: 0 1rem;display: inline-block;line-height: 5rem;height: 5rem;width: 100%;}

/*set-up*/
.cont-list{border-top: 1px solid #ddd;border-bottom: 1px solid #ddd;background-color: #fff;margin-top: 0.8rem;}
.cont-list li+li{border-top: 1px solid #ddd;}
.cont-list li{height: 5rem;}
.list-link{padding: 0 1.5rem;display: inline-block;height:100%;line-height: 5rem;width: 100%;}
.list-text{font-size: 1.4rem;color:#333;width: 30%;}
.list-more{width: 70%;}
.btn-box{width: 100%;text-align: center;margin-top: 2.3rem;margin-bottom: 2.3rem;}
.btn{width:90%;margin: 5%;border-radius:5px;padding:1.5rem 0;background-color: #ff5350;font-size: 1.6rem;color:#fff;-moz-appearance: none;-webkit-appearance: none;appearance:none;border:none;outline: none;}

/*change-pwd*/
.change-form,.set-form{margin-top: 2.4rem;}
.form-item{border:1px solid #bbb;border-radius: 5px;margin:0 2rem 1.25rem;background-color: #fff;}
.label-item{position: relative;width: 4.5rem;height: 4.5rem;display: inline-block;text-align: center;line-height: 4.5rem;}
.pwd-input{height: 4.5rem;padding-left: 1.7rem;font-size: 1.4rem;display: inline-block;width:calc(100% - 5rem);width:-webkit-calc(100% - 5rem);vertical-align: top;}
.lock-icon{width:1.3rem;height: 1.3rem;background-position:  -17rem -2.25rem;}
.lock-icon-mobile{width:1.3rem;height: 1.5rem;background-position:  -17rem 0;}
.lock-icon-code{width:1.3rem;height: 1.3rem;background-position:  -17rem -4.2rem;}
.label-item::after{content: "";width: 1px;height: 2rem;background-color: #bbb;position: absolute;right: 0;top:1.3rem;}
.error{color:#ff5350;padding-left: 2rem;margin-top: -1rem;font-size: 1.2rem;}

/*set-pay-pwd*/
.code-btn{width:100%;font-size: 1.2rem;background-color: #fff;height: 4.5rem;padding: 0;text-align:center;border-radius: 5px;border:none;color:#333;-moz-appearance: none;-webkit-appearance: none;appearance:none;outline: none;}
.form-codeItem{width:100%;padding: 0 2rem;}
.code-item-1{width:60%;margin: 0 1rem 0 0;}
.code-item-2{width:calc(40% - 1rem);width:-webkit-calc(40% - 1rem);margin: 0;}
.error-code{clear:both;margin-top: -1rem;}
.code-grey-btn{color:#ccc;}

/*change-info*/
.cont-list li.head-li,.cont-list li.introduct-li{height: 8rem;}
.head-link,.introduct-link{line-height: 8rem;}
#userhead{width: 6rem;height: 6rem;border-radius: 6rem;position: relative;left: 6rem;top: 0;z-index: 10;}
.head-input{width: 6rem;height: 6rem;vertical-align: middle;position: relative;z-index: 11;opacity: 0;}
.real-name,.uername{color:#666;font-size: 1.9rem;display: inline-block;vertical-align: middle;text-align: right;height: 4rem;width: 100%;}
.introduct-box{width:69%;height: 100%;}
.user-introduct{border:none;width: 100%;height: 100%;font-size: 1.4rem;resize: none;overflow:hidden;line-height: 1.5;padding: 2rem 0;}
.info-error{margin-top: 0;}

/*id-card*/
.id-list{margin-top: 0;}
.id-tip{font-size: 1.2rem;padding:1.5rem;}
.id-label{color:#333;}
.tip-text{color:#e53e42;}
.idImg-title-1{padding: 0 1.5rem;line-height: 3;}
.id-img-list{background-color: #fff;border-top: 1px solid #ddd;border-bottom: 1px solid #ddd;}
.id-li{padding: 0 1.5rem;}
.idImg-item{margin: 4%;width: 42%;height: 10.3rem;border:1px solid #ddd;border-radius: 5px;overflow:hidden;text-align: center;position: relative;}
.idImg{width:14.5rem;height: 10.3rem;position: relative;}
.idImg-input{width:14.5rem;height: 10.3rem;position: relative;top:-10.3rem;left: 0;opacity: 0;}
.idImg-title-2{font-size: 1.4rem;text-align: left;}
.idImg-2{height: 100%;}
.abs-imgTitle{position: absolute;top:0;left: 0;}

/*assessment*/
.cont-list li.assessment-li{min-height: 12rem;height: auto;}
.icon-crown{width: 2.2rem;height: 2.4rem;background-position: -9.35rem -31.9rem;margin-left: 0.3rem;}
.crown-points{font-size: 1.4rem;color:#ff5350;display: inline-block;vertical-align: middle;margin-left: 0.7rem;}
.icon-crown-active{background-position: -12.5rem -31.9rem;}
.list-num{color:#999;font-size: 1.2rem;line-height:1.5;padding:1rem 0;}
.instrut-img{width: 6rem;height: 5.4rem;}
.instruct-text{border:0;width:calc(100% - 6rem);width:-webkit-calc(100% - 6rem);padding: 0;font-size: 1.2rem;line-height: 1.5;padding:0 1.3rem;height: 5.4rem;}
.upload-btn{width: 6rem;height: 5.4rem;position: absolute;z-index: 11;opacity: 0;top:0;left: 0;}
.upload-img{width: 6.5rem;height: 6.5rem;background: url('../../../image/wx/o2o-shop/upload-pictures.png') 0 0 no-repeat;position: absolute;bottom: 0;left: 0;background-size: cover;z-index: 10;}
.assessment-li .list-link{margin-bottom: 3rem;}

/*all-order*/
.wrapper-nav{padding:0 1rem;background-color: #fff;width: 100%;border-bottom: 1px solid #ddd;position: fixed;top:0;left: 0;z-index: 100;}
.nav-list{width: 44rem;}
.order-nav{width:6rem;font-size: 1.4rem;text-align: center;padding:1rem 0;margin-right: 3rem;}
.order-nav:last-child{margin-right: 0;}
.nav-active{color:#ff5350;position: relative;}
.nav-active::after{content:"";width:100%;height:0.2rem;background-color: #ff5350;position: absolute;left: 0;bottom: 0;}
.order-item{margin-top: 1rem;background-color: #fff;border-top: 1px solid #ddd;border-bottom: 1px solid #ddd;}
.order-top,.order-shop{line-height: 3.9rem;}
.order-top{padding:0 1rem;border-bottom: 1px solid #ddd;}
.order-state{color:#ff5350;font-size: 1.2rem;}
.order-link{display: inline-block;padding: 0 1rem;font-size: 1.2rem;border-bottom: 1px solid #ddd;padding-bottom: 1.5rem;width: 100%}
.order-id{line-height: 3.7rem;color:#999;}
.order-img{width: 6rem;height: 5.3rem;}
.order-words{position: relative;height: 5.3rem;padding-left: 1.2rem;width:calc(100% - 6rem);width:-webkit-calc(100% - 6rem);}
.order-title{color:#333;line-height: 1.5;}
.order-info{position: absolute;bottom: 0;right: 0;}
.order-num{margin-right: 0.5rem;line-height: 1.8rem;vertical-align: top;}
.order-money{color:#ff5350;font-size: 1.6rem;line-height: 1.8rem;}
.order-bottom{padding: 0 1rem;line-height: 4.5rem;}
.order-label{font-size: 1.2rem}
.label-right{color:#ff5350;}
.pay-btn,.assess-btn{display: inline-block;font-size: 1.3rem;color:#fff;background-color: #ff5350;padding:0.8rem 2.4rem;line-height: 1;border-radius: 5px;}
.assess-btn{background-color: #fff;color:#ff5350;border:1px solid #ff5350;}
.slide-box{padding-top: 3.5rem;position: relative;}
.slide-up,.slide-down{text-align: center;margin-top: 1rem;}
.slideUp-label{font-size:1.2rem;}
.loading{position:fixed;top:50%;left: 50%;background-color:rgba(0,0,0,.8);padding:1rem;border-radius: 5px;margin-left: -2.5rem;margin-top: -2.5rem;display: none;}
.loadImg{width: 4rem;height: 4rem;}

/*my-wallet*/
.w-money-1{font-size: 3.6rem;color:#fff;}
.wallet-text{font-size: 1.2rem;color: #fff;}
.walletMoney{position: relative;display: inline-block;}
.wallet-top{background:url('../../../image/wx/o2o-shop/banner-wallet.png') 0 0 no-repeat;width: 100%;height: 20rem;-webkit-background-size:100% 100%;background-size:100% 100%;position: relative;}
.w-box1{text-align: center;padding-top:3.2rem;}
.w-money-1{margin-top: 1.8rem;}
.w-box2{position: absolute;bottom:1.3rem;left: 0;width: 100%;}
.w2-left,.w2-right{width: 50%;text-align: center;position: relative;}
.w2-left::after{content: "";width:1px;height:100%;background-color: #ff9391;position: absolute;right: 0;top: 0;}
.w2-text{color:#ffbaba;font-size: 1.2rem;padding-bottom: 1rem;}
.w2-num,.w2-money{font-size: 1.6rem;color:#fff;}
.rmb-icon{font-size: 1.2rem;color:#fff;}
.wallet-middle{background-color: #fff}
.wm-item{width:33.33%;text-align: center;margin:1rem 0;}
.wm-item+.wm-item{border-left: 1px solid #ddd;}
.wm-text{font-size: 1.2rem;color:#333;line-height: 1.5;}
.wm-money{font-size: 1.6rem;color:#ff5350;text-overflow: ellipsis;overflow: hidden;white-space: nowrap;line-height: 1.5;}
.wm-money .rmb-icon{color:#ff5350;}
.wallet-menu{border-top: 1px solid #ddd;margin-top: 1rem;}
.wmenu-item{background-color: #fff;display: inline-block;width: 33.3%;text-align: center;font-size: 1.5rem;color:#333;border-bottom: 1px solid #ddd;border-right: 1px solid #ddd;padding:1.5rem 0;}
.wmenu-img{width:4.6rem;height: 3.8rem;margin: 1rem auto;padding: 0.5rem}
.wmenu-img img{max-width: 100%;max-height: 100%;}
.wmenu-item:nth-child(3n){border-right: none;}
.wallet-tip{width: 100%;background-color: #fffce0;text-align: center;border-bottom: 1px solid #f0cd56;padding:0.9rem 0;}
.wtip-center{color:#c58d13;font-size: 1.2rem;}

/*withdraw-cash*/
.cash-month,.cash-time{font-size: 1.4rem;}
.cash-month{padding: 1rem 1.5rem;}
.cash-list{background-color:#fff;}
.cash-li{text-align: center;padding: 1.3rem 0;height: 7.5rem;margin: 0 1.5rem;}
.cash-li+.cash-li{border-top: 1px solid #efefef;}
.cash-time{line-height: 2.5rem;}
.cash-icon{background:url('../../../image/wx/o2o-shop/cash-1.png') 0 0 no-repeat;width: 3.5rem;height: 3.5rem;background-size: cover;margin-top: 0.5rem;margin:0.5rem 2.5rem;}
.cash-text{font-size: 1.6rem;line-height: 2.5rem;text-align: left;}
.cash-state{font-size: 1.2rem;line-height: 2.5rem;text-align: left;}
.cash-red{color:#ff5350;}
.cash-grey{color: #999;}
.cash-green{color:#08c8c6;}

/*lock-card*/
.form-top-list,.form-list{margin-top: 1rem;border-top:1px solid #e5e5e5;border-bottom:1px solid #e5e5e5;}
.form-row{width: 100%;background-color: #fff;line-height: 6rem;height: 6rem;padding:0 1rem;font-size:0;}
.form-row+.form-row{border-top:1px solid #e5e5e5;}
.form-label{display:inline-block;width: 6.2rem;text-align: right;font-size: 1.5rem;}
.form-top-list i{vertical-align: baseline;}
.row-input,.form-select{margin-left: 3rem;font-size: 1.5rem;}
.row-input{width:calc(100% - 9.2rem);width:-webkit-calc(100% - 9.2rem);}
.form-select{width:calc(100% - 10.6rem);width:-webkit-calc(100% - 10.6rem);-webkit-appearance: none;-moz-appearance: none;appearance:none;border:none;height: 100%;}
.agreement-box{width: 100%;text-align: center;margin-top: 1.3rem;}
.agree-link{display: inline-block;font-size: 1.2rem;color:#999;}
.tip-box{width: 100%;background-color: #fffce0;text-align: center;border-bottom: 1px solid #f0cd56;}
.tip-box .tip-cont{color:#c58d13;font-size: 1.2rem;padding: 0.8rem 0;display: inline-block;}
.month-left,.month-right{font-size: 1.2rem;color:#999;letter-spacing: 0.04em;}

/*fenghong-history*/
.red-text{font-size: 1.5rem;}
.red-date{font-size: 1.2rem;color:#999;}
.red-num{font-size: 1.5rem;color:#e53e42;margin-top: 1.5rem;}

/*my-bankCard*/
.bank-list{padding:2rem 1.5rem 0;}
.bank-li{width: 100%;}
.bank-link{display: inline-block;width: 100%;padding: 1.25rem;border-radius: 5px;margin-bottom: 1rem;}
.bank-top{margin-bottom: 1.5rem;}
.bank-icon{width: 3rem;height: 3rem;border-radius: 3rem;}
.bank-name,.bank-type{font-size: 1.6rem;color:#fff;display: inline-block;vertical-align: middle;margin-left: 1.5rem;}
.bank-num{margin-left: 4.8rem;color:#fff;font-size: 1.5rem;letter-spacing: 0.5em;}
@media screen and (max-width:320px) {/*i5*/
	.bank-num{letter-spacing: 0.3em;}
}
.btn-add{width: 100%;padding:0 1.5rem;text-align: center;}
.bank-btn{display:inline-block;color:#ff5350;font-size: 1.5rem;width: 100%;background-color: #fff;border:none;outline:none;padding:1.75rem 0;border:1px solid #ddd;border-radius:5px;}
.icon-add{margin-right: 0.5rem;}

/*shop-cart*/
.null-cart{text-align: center;}
.null-cart-img{width: 17%;margin-top: 5rem;}
.cart-text{font-size: 1.4rem;margin: 2rem 0;}
.null-cart-btn{width: 100%;text-align: center;}
.cartBack-link{display: inline-block;padding: 1rem 2rem;font-size: 1.4rem;color:#e53e42;border:1px solid #e53e42;border-radius: 2rem;}
.shop-cart{padding-top: 0.8rem;}

/*shop-introduce*/
.shop-btn{position: fixed;bottom: 0;left: 0;width: 100%;z-index: 100;}
.edit-btn{width: 100%;display:inline-block;text-align:center;font-size: 1.6rem;color:#fff;-webkit-appearance: none;appearance: none;outline: none;background-color: #ff5350;border:none;padding:1.4rem 0;}
.shop-box{padding: 1rem 1rem 0;background-color: #fff;margin-top: 1rem;margin-bottom: 4.4rem;}
.shop-text{font-size: 1.4rem;color:#666;line-height: 2.25rem;padding:1rem 0;}
.shop-show{width: 100%;height: 15rem;}

/*edit-shop*/
.editShop-form{padding: 1rem 1rem 0;}
.shopShow-input{position: relative;width: 100%;height: 15rem;}
.shopShowImg{position: absolute;top:0;left: 0;z-index: 10;width: 100%;height: 100%;}
.uploadShop-btn{position: absolute;top:0;left: 0;z-index:11;width: 100%;height: 100%;opacity: 0;}
.shop-textarea{margin: 1rem 0;border:none;font-size: 1.4rem;color:#666;line-height: 2.25rem;width: 100%;padding:1rem;height: 30rem;}

/*open-apply(personal)*/
.openShop-form .shopInfo-title{padding:1.5rem;color:#e53e42;font-size: 1.4rem;border-bottom: none;text-align: left;}
.grey-tip{color:#999;font-size: 1.2rem;}
.shopInfo-list{font-size: 1.4rem;margin-top: 0;}
.shopInfo-list li{padding:0 1.5rem;line-height: 5rem;}
.shopInfo-label{display: inline-block;color:#333;}
.shopInfo-right{width: 50%;height: 100%;display: inline-block;font-size: 0;}
.shopInfo-input{text-align: right;display: inline-block;vertical-align: middle;width: 100%;font-size: 1.4rem;}
.choose-city,.approve-state{width:calc(100% - 1.4rem);width:-webkit-calc(100% - 1.4rem);}
.Id-approve{margin-top: 1rem;border-top: 1px solid #ddd;border-bottom: 1px solid #ddd;height: 5rem;line-height: 5rem;background-color: #fff;padding: 0 1.5rem;font-size: 1.4rem;}
.approve-link{width: 100%;line-height: 5rem;display: inline-block;font-size: 0;}
.approve-state{font-size: 1.4rem;text-align: right;font-size: 1.4rem;}
.approve-not{color:#ff5350;}
.approve-finish{color:#867f7f;}
.shopInfo-right i{vertical-align: middle;}

/*open-apply(company)*/
.register-money{width:calc(100% - 3.8rem);width:-webkit-calc(100% - 3.8rem);}
.money-unit{font-size: 1.4rem;margin-left: 1rem;}
.openShop-form,.release-form{position: relative;}
.apply-wrap-1,.release-wrap-1{position: absolute;top:0;left: 0;width: 100%;transition: left .3s;-webkit-transition: left .3s;}
.apply-wrap-2,.apply-wrap-3,.release-wrap-2{position: absolute;top:0;left:100%;width: 100%;transition: left .3s;-webkit-transition: left .3s;}
.apply-wrap-active,.release-wrap-active{left: 0;}
.apply-wrap-left,.release-wrap-left{left: -100%;}
.apply-wrap-right,.release-wrap-right{left: 100%;}

/*apply-finance(company)*/
.icon-no,.icon-yes{width: 1.45rem;height: 1.45rem;background-position: -22.8rem 0;}
.icon-yes{background-position: -22.8rem -2.2rem;}
.choose-text{font-size: 1.2rem;color:#333;}
.choose-btn{display: inline-block;}

/*apply-license(company)*/
.license-list{font-size: 1.4rem}
.license-list li{padding:0 1.5rem;line-height: 5rem;}
.license-tip{color:#999;font-size: 1.2rem;line-height: 1.2;}
.license-startTime,.license-middle,.license-endTime{font-size: 1.4rem;width:calc(50% - 0.7rem);width:-webkit-calc(50% - 0.7rem);}
.license-middle{width: 1.4rem;text-align: center;}
.license-startTime,.license-endTime{vertical-align:middle;text-align: center;}
