	height: 4rem;
	line-height: 4rem;
	position: relative;
}
.topTitle a{
	display: inline-block;
	width:4.4rem;
	height:4rem;
	position: absolute;
	left: 1rem;
	top: 0;
}
.topTitle a::before{
	content:"";
	position: absolute;
	top: 0.9rem;
	left: 0;
	width: 1.1rem;
	height: 2rem;
	background-repeat: no-repeat;
	-webkit-background-size: contain;
	background-size: contain;
	background-image: url(data:image/png;base64,);
